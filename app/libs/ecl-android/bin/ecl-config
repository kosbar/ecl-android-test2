#!/bin/sh
# bin/ecl-config.pre.  Generated from ecl-config by configure.

usage()
{
  cat <<EOF
Usage: $0 [OPTIONS] [LIBS]
Options: 
	[--cflags]
	[--libs|--ldflags]
Libs:
	cmp
EOF
}

LDFLAGS="-lecl"
for i in $*; do
  case $i in
    --cflags|-c)
      echo_cflags=yes
      ;;
    --libs|--ldflags|-l)
      echo_ldflags=yes
      ;;
    cmp)
      LDFLAGS="$LDFLAGS -lcmp"
      ;;
    *)
      usage 1 >&2
      ;;
    esac;
done

if test "$echo_cflags" = "yes"; then
  echo "-Dandroid   -I/home/kosbar/Projects/ecl/ecl-android/include"
fi

if test "$echo_ldflags" = "yes"; then
  echo "-Wl,--rpath,/home/kosbar/Projects/ecl/ecl-android/lib -L/home/kosbar/Projects/ecl/ecl-android/lib $LDFLAGS --sysroot=/home/kosbar/Android/Sdk/ndk/27.0.12077973/toolchains/llvm/prebuilt/linux-x86_64/sysroot -D__ANDROID_API__=23     -ldl -lm  "
fi
